{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "net-watch",
  "version": "1.0.0",
  "identifier": "com.pamod.net-watch",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "net-watch",
        "width": 1200,
        "height": 800
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": ["icons/icon.ico","icons/icon.png"]
  }
}